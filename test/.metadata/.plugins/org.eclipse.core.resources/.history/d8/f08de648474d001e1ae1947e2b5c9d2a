package tut00;

//클래스 이름은 Hello 파일이름과 동일해야
public class Hello {

	public static void main(String[] args) {
		// 한글자는 문자 '', 한글자 이상은 문자열 ""
		
		// : (콜론) => 계속 이어질 무언가 있는 것
		// ; (세미콜론) => 문자 끝, 프로그램 끝
		System.out.println("헬로");
		System.out.println("월");
	}

}
